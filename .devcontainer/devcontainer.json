{
  "name": "ARTSEY QMK Development",
  "image": "qmkfm/qmk_cli",
  "runArgs": ["--security-opt", "label=disable"],
  "mounts": [
    "type=bind,source=${localWorkspaceFolder}/.build,target=/qmk_firmware/.build",
    "type=bind,source=${localWorkspaceFolder}/keyboards/artsey,target=/qmk_firmware/keyboards/artsey",
    "type=bind,source=${localWorkspaceFolder}/users/artsey,target=/qmk_firmware/users/artsey"
  ],
  "workspaceMount": "type=volume,source=qmk_firmware,target=/qmk_firmware",
  "workspaceFolder": "/qmk_firmware",
  "extensions": [
    "ms-vscode.cpptools",
    "EditorConfig.EditorConfig"
  ]
}
